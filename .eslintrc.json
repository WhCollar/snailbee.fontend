{
  "plugins": [
    "boundaries",
    "import"
  ],
  "extends": [
    "plugin:prettier/recommended",
    "@vue/eslint-config-typescript",
    "plugin:vue/vue3-essential",
    "@feature-sliced"
  ],
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "@typescript-eslint/parser"
  },
  "settings": {
    "import/resolver": {
      "typescript": {
        "alwaysTryTypes": true
      }
    }
  },
  "ignorePatterns": [
    "**/typicode/generated/*"
  ],
  "rules": {
    "vue/multi-word-component-names": [
      "off"
    ]
  }
}